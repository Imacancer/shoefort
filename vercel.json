{
    "version": 2,
    "builds": [
      { "src": "/shoefort/server/index.mjs", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/auth", "dest": "/shoefort/server/routes/authRoutes.js" },
      { "src": "/products", "dest": "/shoefort/server/routes/productRoutes.js" },
      { "src": "/customers", "dest": "/shoefort/server/routes/customerRoutes.js" },
      { "src": "/checkout", "dest": "/shoefort/server/routes/checkoutRoutes.js" },
      { "src": "/transactions", "dest": "/shoefort/server/routes/transactionRoutes.js" },
      { "src": "/services", "dest": "/shoefort/server/routes/servicesRoutes.js" }
    ]
  }
  