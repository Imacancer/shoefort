{
    "version": 2,  
    "builds": [
      {
        "src": "/server/index.mjs",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/server/index.mjs",
        "dest": "/server/index.mjs"
      },
      {
        "src": "/server/routes/(.*)",
        "dest": "/server/routes/$1"
      }
    ]
  }
  